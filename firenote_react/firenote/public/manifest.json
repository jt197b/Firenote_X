{
  "manifest_version": 2,
  "name": "Firenote: Notes and Todo Lists in New Tab",
  "description": "Create, edit, and save notes and todo lists every time you open a new tab.",
  "version": "3.0.0",
  
  "chrome_url_overrides" : {
      "newtab": "index.html"
  },
  
  "browser_action": {
      "default_icon": "images/icon128.png",
      "default_popup": "index.html",
      "default_title": "Open the popup"
  },
  
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
   "128": "images/icon128.png"
  },

  "content_security_policy": "script-src 'self' 'sha256-X14Dv/JPjbpTTTJyAX3I1IqxLWHzOjtXtjphkt/1lsc=' 'unsafe-eval'; object-src 'self'",

  "permissions": [
  "storage"
  ]
  }